{
  "expo": {
    "name": "Bigyeong",
    "slug": "bigyeong",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo1.png",
    "splash": {
      "image": "./logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
